<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.maibaduoduo.store.dao.StoreDao">

    <!-- 映射 -->
    <resultMap type="com.maibaduoduo.store.entity.StoreEntity" id="storeMap">
                    <result property="id" column="id"/>
                    <result property="storeNo" column="store_no"/>
                    <result property="storeName" column="store_name"/>
                    <result property="storeDescription" column="store_description"/>
                    <result property="storeArea" column="store_area"/>
                    <result property="storeQuantity" column="store_quantity"/>
                    <result property="storeAreaCode" column="store_area_code"/>
                    <result property="storeAreaName" column="store_area_name"/>
                    <result property="ensureCapacity" column="ensure_capacity"/>
                    <result property="createUserId" column="create_user_id"/>
                    <result property="createTime" column="create_time"/>
                    <result property="tenantId" column="tenant_id"/>
            </resultMap>

</mapper>